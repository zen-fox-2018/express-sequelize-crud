<!DOCTYPE html>
<html lang="en">
<head>
    <title>Students List</title>
    <style>
        table {
            border: 1px solid rgb(0, 0, 0);
            background: white;
        }
        th, td {
            padding: 15px;
            text-align: left;
            border: 1px solid rgb(0, 0, 0);
        }
        /* body { */
            /* background-image: url("https://images.pexels.com/photos/448877/pexels-photo-448877.jpeg?auto=compress&cs=tinysrgb&dpr=1&w=500"); */
            /* background-size: cover; */
        /* } */
        th {
            background: Yellow;
        }
        #Judul {
            color: greenyellow;
        }
        /* h1 h5 {
            margin: auto 0;
            text-align: center;
        } */
    </style>
</head>
<body>
    <h1> <%= title %> </h1>
    <h5><a href="/students/add">add student</a></h5>
    <table>
        <tr>
            <th>id</th> 
            <th>first_name</th>
            <th>last_name</th>
            <th>email</th>
        </tr>
        <% allData.forEach(e => { %>
        <tr>    
            <td><%= e.id %></td>
            <td><%= e.first_name %></td>
            <td><%= e.last_name %></td>
            <td><%= e.email %></td>
        </tr>
        <% }) %>
    </table>
</body>
</html>