<%- include ./partial/header.ejs %>
<div class="container">
    <h2 style="text-align: center">Subjects Data</h2>
    <h4 style="text-align :center"><a href="/subject/add"> Add Subjects Data</a></h4>
    <table class="table table-hover">
      <thead>
        <tr>
          <th>id</th>
          <th>Subject Name</th>
          <th>Option</th>
        </tr>
      </thead>
      <tbody>
        <% for (let i = 0 ; i < data.length; i++ ) { %>
          <tr>
            <td><%= data[i].id %></td>
            <td><%= data[i].subject_name %></td>
            <td><a href="/subject/edit/<%= data[i].id %>"> Edit </a> | 
              <a onclick="return confirm('Are you sure')"  href="/subject/delete/<%= data[i].id %>"> Delete </a></td>
          </tr>
        <% } %>
      </tbody>
    </table>
  </div>
<%- include ./partial/footer.ejs %>